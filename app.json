{
  "expo": {
    "name": "SHJP",
    "description": "Sacred Heart of Jesus Parish Youth Mobile Application",
    "slug": "shjp-mobile",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0-alpha",
    "orientation": "portrait",
    "icon": "./res/logo.png",
    "splash": {
      "image": "./res/logo_xxxhdpi.png",
      "xxxhdpi": "./res/logo_xxxhdpi.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shjp.shjp"
    },
    "android": {
      "package": "com.shjp.shjp",
      "permissions": []
    }
  }
}
